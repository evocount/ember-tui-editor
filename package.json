{
  "name": "ember-tui-editor",
  "version": "0.1.6",
  "description": "TOAST UI WYSIWYG markdown editor for Ember.",
  "keywords": [
    "ember-addon",
    "ember",
    "markdown",
    "toast ui",
    "wysiwyg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evocount/ember-tui-editor"
  },
  "license": "MIT",
  "author": "Till Theato <till.theato@evocount.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy:demo": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "@toast-ui/editor": "^2.1.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^5.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release-tag": "^0.1.5",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^4.0.0",
    "ember-prism": "^0.6.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.19.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.0.0",
    "eslint-plugin-ember": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://evocount.github.io/ember-tui-editor/"
  }
}
